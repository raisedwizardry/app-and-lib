{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "saying-hello": {
      "projectType": "library",
      "root": "projects/saying-hello",
      "sourceRoot": "projects/saying-hello/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/saying-hello/tsconfig.lib.json",
            "project": "projects/saying-hello/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/saying-hello/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/saying-hello/src/test.ts",
            "tsConfig": "projects/saying-hello/tsconfig.spec.json",
            "karmaConfig": "projects/saying-hello/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/saying-hello/tsconfig.lib.json",
              "projects/saying-hello/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "saying-goodbye": {
      "projectType": "library",
      "root": "projects/saying-goodbye",
      "sourceRoot": "projects/saying-goodbye/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/saying-goodbye/tsconfig.lib.json",
            "project": "projects/saying-goodbye/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/saying-goodbye/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/saying-goodbye/src/test.ts",
            "tsConfig": "projects/saying-goodbye/tsconfig.spec.json",
            "karmaConfig": "projects/saying-goodbye/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/saying-goodbye/tsconfig.lib.json",
              "projects/saying-goodbye/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "introduction": {
      "projectType": "library",
      "root": "projects/introduction",
      "sourceRoot": "projects/introduction/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/introduction/tsconfig.lib.json",
            "project": "projects/introduction/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/introduction/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/introduction/src/test.ts",
            "tsConfig": "projects/introduction/tsconfig.spec.json",
            "karmaConfig": "projects/introduction/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/introduction/tsconfig.lib.json",
              "projects/introduction/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "interaction": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/interaction",
      "sourceRoot": "projects/interaction/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/interaction",
            "index": "projects/interaction/src/index.html",
            "main": "projects/interaction/src/main.ts",
            "polyfills": "projects/interaction/src/polyfills.ts",
            "tsConfig": "projects/interaction/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/interaction/src/favicon.ico",
              "projects/interaction/src/assets"
            ],
            "styles": [
              "projects/interaction/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/interaction/src/environments/environment.ts",
                  "with": "projects/interaction/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "interaction:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "interaction:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "interaction:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/interaction/src/test.ts",
            "polyfills": "projects/interaction/src/polyfills.ts",
            "tsConfig": "projects/interaction/tsconfig.spec.json",
            "karmaConfig": "projects/interaction/karma.conf.js",
            "assets": [
              "projects/interaction/src/favicon.ico",
              "projects/interaction/src/assets"
            ],
            "styles": [
              "projects/interaction/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/interaction/tsconfig.app.json",
              "projects/interaction/tsconfig.spec.json",
              "projects/interaction/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/interaction/e2e/protractor.conf.js",
            "devServerTarget": "interaction:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "interaction:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "saying-hello"
}